<odoo>
    <record id="view_move_form_encf" model="ir.ui.view">
        <field name="name">account.move.form.inherit.encf</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <group>
                    <field name="encf_type" placeholder="Select an ENCF Type"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>